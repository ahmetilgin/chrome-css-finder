{  "name": "Ä°lk Eklentim", 
     "version": "1.0", 
    "manifest_version": 2, 
    "description": "Bu bir google chrome eklentisidir.", 
    "browser_action": { 
        "default_icon": "css.png",
        "default_popup" : "index.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
      ],
          "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["main.js"],
        "run_at": "document_end",
        "all_frames": true
      }
    ]
  
}
